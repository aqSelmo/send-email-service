{
  "name": "docker-kafka",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "docker:stop": "docker-compose down",
    "docker:dev": "docker-compose --name app_1 --f docker-compose.dev.yml up -d",
    "docker:test": "docker-compose --f docker-compose.test.yml up --exit-code-from app",
    "docker:bash": "docker exec --it app_1 /bin/bash",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "start:dev": "ts-node-dev --poll --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "test": "jest --runInBand"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.14",
    "@babel/node": "^7.13.13",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-typescript": "^7.13.0",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.21",
    "@types/mongodb": "^3.6.11",
    "@types/node": "^14.14.35",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "express": "^4.17.1",
    "mongodb": "^3.6.5",
    "reflect-metadata": "^0.1.13",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "typeorm": "^0.2.31"
  }
}
